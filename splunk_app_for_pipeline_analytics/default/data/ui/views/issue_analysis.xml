          <eval token="selected_issue_tags">if('search_issue_tags'=="*" OR isnull('search_issue_tags'), "", 'selected_issue_tags_spl')</eval>
          <eval token="selected_issue_tags_spl2">replace('selected_issue_tags', "^Work_Issues\\.", "")</eval>
